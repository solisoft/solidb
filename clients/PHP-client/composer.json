{
    "name": "solidb/php-client",
    "description": "Native PHP client for SoliDB",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SoliDB\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4",
        "rybakit/msgpack": "^0.9.0"
    },
    "suggest": {
        "ext-msgpack": "Native PHP extension for better performance"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
